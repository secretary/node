{
  "name": "@secretary/cli",
  "version": "4.0.0",
  "description": "Secretary CLI",
  "author": "Secretary",
  "bin": {
    "secretary": "./bin/run"
  },
  "homepage": "https://github.com/secretary/node",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "secretary/node",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.21.0",
    "@oclif/plugin-help": "^5",
    "@secretary/core": "^4.0.0",
    "execa": "5",
    "yup": "1.0.0-beta.8"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.13",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.7",
    "chai": "^4",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-plugin-unicorn": "^45.0.2",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "oclif": {
    "bin": "secretary",
    "dirname": "secretary",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "inject": {
        "description": "Inject secrets into a command"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint --fix --config=eslint.json src",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": "18"
  },
  "bugs": "https://github.com/secretary/node/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
