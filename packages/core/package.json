{
  "name": "@secretary/core",
  "description": "Secrets Manager for NodeJS",
  "version": "4.0.6",
  "author": "Aaron Scherer <aequasi@gmail.com>",
  "dependencies": {
    "lru-cache": "7.14.1",
    "source-map-support": "0.5.21"
  },
  "devDependencies": {
    "@types/chai-as-promised": "7.1.5",
    "@types/eslint": "8.4.10",
    "@types/lru-cache": "7.10.10",
    "@types/source-map-support": "0.5.6",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "chai-as-promised": "7.1.1",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "typemoq": "2.1.0"
  },
  "gitHead": "c93155fcbe9666a90f19c175864e9f52c1223611",
  "license": "MIT",
  "main": "dist/index.js",
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "reporter": [
      "text"
    ],
    "all": true
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "secretary/node",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && npx codecov",
    "lint": "eslint --fix --config=eslint.json src",
    "prepare": "npm run build",
    "test": "nyc mocha"
  },
  "types": "dist/index.d.ts"
}
