{
  "name": "@secretary/json-file-adapter",
  "description": "JSON File adapter for Secretary",
  "version": "4.0.0",
  "author": "Aaron Scherer <aequasi@gmail.com>",
  "dependencies": {
    "@secretary/core": "4.0.0"
  },
  "devDependencies": {
    "@types/chai-as-promised": "7.1.5",
    "@types/eslint": "8.4.10",
    "@types/mock-fs": "4.13.1",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "chai-as-promised": "7.1.1",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "mock-fs": "5.2.0",
    "typemoq": "2.1.0"
  },
  "gitHead": "1a22d1f11bd050b501dd3974228caace9f092e1e",
  "license": "MIT",
  "main": "dist/index.js",
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "reporter": [
      "text"
    ],
    "all": true
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "secretarysecrets/node-json-file",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && npx codecov",
    "lint": "eslint --fix --config=eslint.json src",
    "test": "nyc mocha"
  },
  "typings": "dist/index.d.ts"
}
